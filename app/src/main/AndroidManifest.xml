<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yt.khadamat">

    <application
        android:allowBackup="true"
        android:icon="@drawable/khadamat"
        android:label="@string/app_name"
        android:roundIcon="@drawable/khadamat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login_register.register"></activity>
        <activity android:name=".login_register.login" />
        <activity
            android:name=".Profile"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".menu.plumber.plumber" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>